cp /TheP8I/Handlers/StringFrag/StringFragmenter dummy
insert /TheP8I/StdStrategy:DefaultObjects[0] dummy
set dummy:StringZ_epsilonC 0.051
set /TheP8I/Handlers/StringFrag/StringFragmenter:StringZ_epsilonC 0.049

saverun TestJunctions SimpleLEPGenerator
