cd /Pythia7/Generators
cp StdLHC StdTevatron
cp /Pythia7/Handlers/Event/StdLHC /Pythia7/Handlers/Event/TevatronJets
set StdTevatron:EventHandler /Pythia7/Handlers/Event/TevatronJets
cp /Pythia7/Handlers/Lumi/LHC /Pythia7/Handlers/Lumi/TevatronRun1
set /Pythia7/Handlers/Lumi/TevatronRun1:BeamEMaxA 900.0
set /Pythia7/Handlers/Lumi/TevatronRun1:BeamEMaxB 900.0
set StdTevatron:EventHandler:LuminosityFunction /Pythia7/Handlers/Lumi/TevatronRun1
set /Pythia7/Handlers/Event/TevatronJets:BeamA /Pythia7/Particles/pbar-
set /Pythia7/Handlers/Event/TevatronJets:BeamB /Pythia7/Particles/p+

set StdTevatron:NumberOfEvents 1000
set StdTevatron:DebugLevel 1
set StdTevatron:PrintEvent 10
create ThePEG::HZTool HZTest HZTool.so
insert HZTest:Functions[0] hzh9807018
insert StdTevatron:AnalysisHandlers[0] HZTest
create ThePEG::GraphvizPlot Plotter GraphvizPlot.so
set Plotter:Quiet true
insert StdTevatron:AnalysisHandlers[0] Plotter
saverun TestHZTool StdTevatron
