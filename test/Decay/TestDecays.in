cd /Pythia7/Generators
set StdLEP:NumberOfEvents 10000
set StdLEP:DebugLevel 1
set StdLEP:PrintEvent 10
cp /Defaults/Particles/Z0 /Pythia7/Particles/Z0
set /Pythia7/Particles/Z0:NominalMass 92
insert /Pythia7/StdStrategy:LocalParticles[0] /Pythia7/Particles/Z0
set /Defaults/Particles/pi0:Stable true
mset /Defaults/Particles/D_s+ ThePEG::DecayMode OnOff Off
set /Defaults/Particles/D_s+/D_s+->s=sbar,u=dbar;:OnOff On
set /Defaults/Particles/D_s+/D_s+->u=dbar;:OnOff On
set /Defaults/Particles/u:NominalMass 0.33
set /Defaults/Particles/d:NominalMass 0.33
set /Defaults/Particles/s:NominalMass 0.5
set /Defaults/Particles/c:NominalMass 1.5
set /Defaults/Particles/b:NominalMass 4.8

saverun TestDecays StdLEP
