// -*- C++ -*-
//
// This is the implementation of the inlined member functions of
// the HardRemnant class.
//

namespace Ariadne {

inline HardRemnant::HardRemnant()
  : theQ2(0.0*GeV2) {}

inline HardRemnant::HardRemnant(const HardRemnant & x)
  : RemnantParton(x), theQ2(x.theQ2) {}

inline Energy2 HardRemnant::Q2() const{
  return theQ2;
}

inline void HardRemnant::setQ2(Energy2 Q2){
  theQ2 = Q2;
}

}
