mkdir /Ariadne5/MultiLEP
cd /Ariadne5/MultiLEP
#cp /Ariadne5/Generators/LEP/LEPGenerator SimpleLEPGenerator
create ThePEG::MultiEventGenerator MultiLEPGenerator MultiEventGenerator.so
set MultiLEPGenerator:EventHandler /Ariadne5/Generators/LEP/LEPGenerator:EventHandler
set MultiLEPGenerator:RandomNumberGenerator /Ariadne5/Generators/LEP/LEPGenerator:RandomNumberGenerator
set MultiLEPGenerator:StandardModelParameters /Ariadne5/Generators/LEP/LEPGenerator:StandardModelParameters
set MultiLEPGenerator:NumberOfEvents 100000
set MultiLEPGenerator:DebugLevel 0
set MultiLEPGenerator:PrintEvent 0
create ThePEG::RivetAnalysis RivetLEP RivetAnalysis.so
# insert RivetLEP:Analyses[0] DELPHI_1995_S3137023
insert RivetLEP:Analyses[0] DELPHI_1996_S3430090
#insert RivetLEP:Analyses[0] OPAL_2001_S4553896
#insert RivetLEP:Analyses[0] OPAL_2004_S6132243
#insert RivetLEP:Analyses[0] ALEPH_1991_S2435284
#insert RivetLEP:Analyses[0] ALEPH_1996_S3486095
#insert RivetLEP:Analyses[0] ALEPH_2004_S5765862
insert MultiLEPGenerator:AnalysisHandlers[0] RivetLEP

# set /Ariadne5/Particles/pi0:Stable Unstable
cp MultiLEPGenerator MultiLEPGenerator3
cp MultiLEPGenerator MultiLEPGenerator4
cp MultiLEPGenerator MultiLEPGenerator4sw
do MultiLEPGenerator:AddInterface /Ariadne5/Defaults/Frag8:StringZ_aLund 0.10, 0.23, 0.6
do MultiLEPGenerator:AddInterface /Ariadne5/Defaults/Frag8:StringZ_bLund 0.20, 0.34, 0.9
do MultiLEPGenerator:AddInterface /Ariadne5/Defaults/Frag8:StringPT_sigma 0.2, 0.405, 0.6
do MultiLEPGenerator:AddInterface /Ariadne5/Cascade/AriadneCascade:LambdaQCD 0.18, 0.22, 0.25
do MultiLEPGenerator:AddInterface /Ariadne5/Cascade/AriadneCascade:PTCut 0.4, 0.6, 0.8

saverun MTuneLEP2 MultiLEPGenerator

set MultiLEPGenerator:EventHandler:CascadeHandler:PurgeStrategy BeforeAndAfter
set MultiLEPGenerator:EventHandler:CascadeHandler:PurgeFactor 1.0
create Ariadne5::DipoleSwinger Swinger
do Swinger:SetRmax -2.7
set Swinger:SizeOpt 1
set Swinger:Lambda 1
set Swinger:Linear Logarithmic
set Swinger:RhoCut -1.0
insert MultiLEPGenerator:EventHandler:CascadeHandler:Emitters[0] Swinger
saverun MTuneLEPsw2 MultiLEPGenerator
# The swing increases the hardness and decreases the multiplicity

do MultiLEPGenerator3:AddInterface /Ariadne5/Defaults/Frag8:StringZ_aLund 0.1, 0.3, 0.5, 0.7
do MultiLEPGenerator3:AddInterface /Ariadne5/Defaults/Frag8:StringZ_bLund 0.2, 0.5, 0.8, 1.1
do MultiLEPGenerator3:AddInterface /Ariadne5/Defaults/Frag8:StringPT_sigma 0.2, 0.4, 0.6, 0.8

do MultiLEPGenerator4:AddInterface /Ariadne5/Defaults/Frag8:StringZ_aLund 0.26, 0.2, 0.35, 0.5, 0.7
do MultiLEPGenerator4:AddInterface /Ariadne5/Defaults/Frag8:StringZ_bLund 0.37, 0.3, 0.5, 0.8, 1.1
do MultiLEPGenerator4:AddInterface /Ariadne5/Defaults/Frag8:StringPT_sigma 0.35, 0.3, 0.4, 0.6, 0.8

do MultiLEPGenerator4sw:AddInterface /Ariadne5/Defaults/Frag8:StringZ_aLund 0.35, 0.1, 0.25, 0.45, 0.7
do MultiLEPGenerator4sw:AddInterface /Ariadne5/Defaults/Frag8:StringZ_bLund 0.39, 0.3, 0.5, 0.8, 1.1
do MultiLEPGenerator4sw:AddInterface /Ariadne5/Defaults/Frag8:StringPT_sigma 0.34, 0.3, 0.4, 0.6, 0.8

saverun MTuneLEPsw3 MultiLEPGenerator3

set /Ariadne5/Cascade/BasicConsistency:CheckGluonPT Off

saverun MTuneLEPsw5 MultiLEPGenerator3

erase /Ariadne5/Cascade/Models/FSGluon:Reweighters[0]

saverun MTuneLEPsw4 MultiLEPGenerator3
saverun MTuneLEPsw40 MultiLEPGenerator4sw
do MultiLEPGenerator4sw:RemoveInterface /Ariadne5/Defaults/Frag8:StringZ_aLund
do MultiLEPGenerator4sw:RemoveInterface /Ariadne5/Defaults/Frag8:StringZ_bLund
do MultiLEPGenerator4sw:RemoveInterface /Ariadne5/Defaults/Frag8:StringPT_sigma
set /Ariadne5/Defaults/Frag8:StringZ_aLund 0.35
set /Ariadne5/Defaults/Frag8:StringZ_bLund 0.39
set /Ariadne5/Defaults/Frag8:StringPT_sigma 0.34
saverun	MTuneLEPsw41 MultiLEPGenerator4sw
set /Ariadne5/Particles/pi0:Stable Stable
saverun	MTuneLEPsw42 MultiLEPGenerator4sw

do MultiLEPGenerator4sw:AddRndInterface /Ariadne5/Defaults/Frag8:StringZ_aLund 100 0.05 2.0 0.35 0.2
do MultiLEPGenerator4sw:AddRndInterface /Ariadne5/Defaults/Frag8:StringZ_bLund   1 0.21 2.0 0.40 0.3
do MultiLEPGenerator4sw:AddRndInterface /Ariadne5/Defaults/Frag8:StringPT_sigma  1 0.05 1.0 0.35 0.2
saverun MTuneLEPsw60 MultiLEPGenerator4sw
do MultiLEPGenerator4sw:AddRndInterface /Ariadne5/Defaults/Frag8:StringPT_sigma  1 0.05 1.0 0.34 0.1
do MultiLEPGenerator4sw:AddRndInterface /Ariadne5/Defaults/Frag8:StringZ_aLund 100 0.05 2.0 0.25 0.1
do MultiLEPGenerator4sw:AddRndInterface /Ariadne5/Defaults/Frag8:StringZ_bLund   1 0.21 2.0 0.32 0.15
saverun MTuneLEPsw61 MultiLEPGenerator4sw
do MultiLEPGenerator4sw:AddRndInterface /Ariadne5/Defaults/Frag8:StringPT_sigma  1 0.05 1.0 0.34 0.005
do MultiLEPGenerator4sw:AddRndInterface /Ariadne5/Defaults/Frag8:StringZ_aLund 100 0.05 2.0 0.25 0.05
do MultiLEPGenerator4sw:AddRndInterface /Ariadne5/Defaults/Frag8:StringZ_bLund   1 0.30 2.0 0.32 0.09
saverun MTuneLEPsw62 MultiLEPGenerator4sw
set MultiLEPGenerator4sw:NumberOfEvents 1000000
saverun MTuneLEPsw63 MultiLEPGenerator4sw

do MultiLEPGenerator4sw:RemoveInterface /Ariadne5/Defaults/Frag8:StringZ_aLund
do MultiLEPGenerator4sw:RemoveInterface /Ariadne5/Defaults/Frag8:StringZ_bLund
do MultiLEPGenerator4sw:RemoveInterface /Ariadne5/Defaults/Frag8:StringPT_sigma
set /Ariadne5/Defaults/Frag8:StringZ_aLund 0.28
set /Ariadne5/Defaults/Frag8:StringZ_bLund 0.33
set /Ariadne5/Defaults/Frag8:StringPT_sigma 0.34
set MultiLEPGenerator4sw:NumberOfEvents 10000000
saverun MTuneLEPsw64 MultiLEPGenerator4sw

erase MultiLEPGenerator3:EventHandler:CascadeHandler:Emitters[0]
set MultiLEPGenerator3:EventHandler:CascadeHandler:PurgeStrategy Never

saverun MTuneLEP4 MultiLEPGenerator3
saverun MTuneLEP40 MultiLEPGenerator4

set MultiLEPGenerator3:NumberOfEvents 50000
saverun MTuneLEP5 MultiLEPGenerator3

do MultiLEPGenerator4:AddRndInterface /Ariadne5/Defaults/Frag8:StringZ_aLund 100 0.05 2.0 0.35 0.2
do MultiLEPGenerator4:AddRndInterface /Ariadne5/Defaults/Frag8:StringZ_bLund   1 0.21 2.0 0.40 0.3
do MultiLEPGenerator4:AddRndInterface /Ariadne5/Defaults/Frag8:StringPT_sigma  1 0.05 1.0 0.35 0.2
saverun MTuneLEP60 MultiLEPGenerator4
do MultiLEPGenerator4:AddRndInterface /Ariadne5/Defaults/Frag8:StringPT_sigma  1 0.05 1.0 0.35 0.1
do MultiLEPGenerator4:AddRndInterface /Ariadne5/Defaults/Frag8:StringZ_aLund 100 0.05 2.0 0.13 0.1
do MultiLEPGenerator4:AddRndInterface /Ariadne5/Defaults/Frag8:StringZ_bLund   1 0.21 2.0 0.27 0.15
saverun MTuneLEP61 MultiLEPGenerator4
do MultiLEPGenerator4:AddRndInterface /Ariadne5/Defaults/Frag8:StringPT_sigma  1 0.05 1.0 0.34 0.01
do MultiLEPGenerator4:AddRndInterface /Ariadne5/Defaults/Frag8:StringZ_aLund 100 0.05 2.0 0.082 0.05
do MultiLEPGenerator4:AddRndInterface /Ariadne5/Defaults/Frag8:StringZ_bLund   1 0.24 2.0 0.27 0.05
saverun MTuneLEP62 MultiLEPGenerator4
set MultiLEPGenerator4:NumberOfEvents 1000000
saverun MTuneLEP63 MultiLEPGenerator4

do MultiLEPGenerator4:RemoveInterface /Ariadne5/Defaults/Frag8:StringZ_aLund
do MultiLEPGenerator4:RemoveInterface /Ariadne5/Defaults/Frag8:StringZ_bLund
do MultiLEPGenerator4:RemoveInterface /Ariadne5/Defaults/Frag8:StringPT_sigma
set /Ariadne5/Defaults/Frag8:StringZ_aLund 0.12
set /Ariadne5/Defaults/Frag8:StringZ_bLund 0.26
set /Ariadne5/Defaults/Frag8:StringPT_sigma 0.35
set MultiLEPGenerator4:NumberOfEvents 10000000
saverun MTuneLEP64 MultiLEPGenerator4




