rrmdir /Ariadne
mkdir /Ariadne
cd /Ariadne
library libArCascade.so
create Ariadne::CascadeHandler AriadneCascade
set AriadneCascade:NFlav 0
create ThePEG::Strategy StdStrategy
mkdir /Ariadne/Particles
cp /Defaults/Particles/pi0 /Ariadne/Particles/pi0
set /Ariadne/Particles/pi0:Stable Stable
insert /Ariadne/StdStrategy:DefaultParticlesDirs[0] /Defaults/Particles
insert /Ariadne/StdStrategy:LocalParticles[0] /Ariadne/Particles/pi0

cd /
